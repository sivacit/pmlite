    "transliteration": "2.3.5",
    "ts-pattern": "5.0.8",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie": "0.6.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "date-fns-tz": "2.0.1",
    "dayjs": "1.11.10",
