    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/testing": "^10.0.0",
    "@prisma/client": "5.11.0",
    "@types/bcrypt": "5.0.2",
    "joi": "17.12.2",
    "keyv": "4.5.4",
    "knex": "3.1.0",
    "lodash": "4.17.21",
    "pg": "^8.13.0",
    "pino-http": "9.0.0",
    "pino-pretty": "11.0.0",
